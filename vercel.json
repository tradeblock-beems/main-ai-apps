{
  "functions": {
    "tools/email-hub/app.py": {
      "runtime": "python3.11"
    }
  },
  "builds": [
    {
      "src": "tools/push-blaster/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/tools/email-hub/(.*)",
      "dest": "/tools/email-hub/app.py"
    },
    {
      "src": "/tools/push-blaster/(.*)",
      "dest": "/tools/push-blaster/$1"
    },
    {
      "src": "/tools/push-blaster",
      "dest": "/tools/push-blaster/"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
