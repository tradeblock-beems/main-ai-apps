{
  "version": 2,
  "builds": [
    {
      "src": "tools/email-hub-serverless/app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/tools/email-hub/(.*)",
      "dest": "/tools/email-hub-serverless/app.py"
    },
    {
      "src": "/tools/email-hub",
      "dest": "/tools/email-hub-serverless/app.py"
    }
  ]
} 